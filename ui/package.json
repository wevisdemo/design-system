{
  "name": "@wevisdemo/ui",
  "version": "7.0.2",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run dev:component\" \"npm run dev:stylesheet\"",
    "dev:component": "chokidar \"src/components/**/*.tsx\" --initial -c \"npm run build:mitosis\"",
    "dev:stylesheet": "postcss src/styles/**/*.css --dir styles --watch",
    "build": "npm run build:component && npm run build:stylesheet",
    "build:component": "npm run build:mitosis && npm run build:react && npm run build:svelte && npm run build:vue",
    "build:mitosis": "sh scripts/build-mitosis.sh",
    "build:react": "tsc -p tsconfig.react.json",
    "build:svelte": "svelte-kit sync && svelte-package -i .mitosis/svelte -o svelte --tsconfig tsconfig.svelte.json && svelte-check --workspace svelte --tsconfig ../tsconfig.svelte.json",
    "build:vue": "vite build --config vite.config.vue.ts",
    "build:stylesheet": "postcss src/styles/**/*.css --dir styles --env production"
  },
  "devDependencies": {
    "@builder.io/eslint-plugin-mitosis": "^0.0.15",
    "@builder.io/mitosis": "^0.1.1",
    "@builder.io/mitosis-cli": "^0.1.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.2",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.0.0",
    "cssnano": "^5.0.16",
    "eslint": "^8.25.0",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.8",
    "tailwindcss": "^3.0.15",
    "typescript": "^5.4.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3",
    "vue": "^3.4.21"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wevisdemo/design-systems.git"
  },
  "files": [
    "/react",
    "/svelte",
    "/vue",
    "/styles"
  ]
}
